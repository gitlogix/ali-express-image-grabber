{
  "name": "ali-express-image-grabber",
  "description": "A chrome extension which grabs the images of an item on aliexpress.com",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run pack && npm run remove-evals && npm run zip",
    "start": "webpack --mode=development --env.NODE_ENV=development --progress --hide-modules --hot --watch",
    "lint": "eslint --ext .js,.vue --fix ./src",
    "pack": "webpack --mode=production --env.NODE_ENV=production --progress --hide-modules",
    "remove-evals": "node scripts/remove-evals.js",
    "zip": "node scripts/build-zip.js"
  },
  "dependencies": {
    "bulma-fluent": "^0.3.19",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "archiver": "^3.1.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-vue": "^6.1.2",
    "fast-glob": "^2.2.7",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.1",
    "purgecss-webpack-plugin": "^1.6.0",
    "sass-loader": "^7.3.1",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^1.1.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12",
    "webpack-extension-reloader": "^1.1.4"
  },
  "license": "MIT"
}
